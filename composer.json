{
    "name": "dwoodard/sketch",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Woodard",
            "email": "dustin.woodard@gmail.com",
            "homepage": "https://github.com/dwoodard/sketch"
        }
    ],
    "homepage": "https://github.com/dwoodard/sketch",
    "keywords": ["Laravel", "Sketch"],
    "require": {
        "illuminate/support": "~7|~8",
        "symfony/yaml": "^5.2",
        "laracasts/generators": "^2.0",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "^6.7"
    },
    "autoload": {
        "psr-4": {
            "Dwoodard\\Sketch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dwoodard\\Sketch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dwoodard\\Sketch\\SketchServiceProvider"
            ],
            "aliases": {
                "Sketch": "Dwoodard\\Sketch\\Facades\\Sketch",
                "YAML": "Dwoodard\\Sketch\\Symfony\\Component\\Yaml\\Yaml"

            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
